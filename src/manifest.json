{
  "package": "com.qaui.sample",
  "name": "QaUI组件库",
  "versionName": "1.0.0",
  "versionCode": "1",
  "minPlatformVersion": "1020",
  "icon": "/common/logo.png",
  "features": [
    { "name": "system.prompt" },
    { "name": "system.router" },
    { "name": "system.shortcut" }
  ],
  "config": {
    "logLevel": "debug",
    "designWidth": 360
  },
  "router": {
    "entry": "pages/home",
    "pages": {
      "pages/home": {
        "component": "index"
      },
      "pages/button": {
        "component": "index"
      },
      "pages/container": {
        "component": "index"
      }
    }
  },
  "display": {
    "titleBarBackgroundColor": "#f7f7f7",
    "titleBarTextColor": "#414141",
    "menu": true,
    "pages": {
      "pages/home": {
        "titleBarText": "QaUI组件库",
        "menu": false
      },
      "pages/button": {
        "titleBarText": "按钮 button",
        "menu": false
      },
      "pages/container": {
        "titleBarText": "容器 container",
        "menu": false
      }
    }
  }
}
